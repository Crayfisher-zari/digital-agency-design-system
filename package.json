{
  "name": "digital-agency-design-system-vue",
  "version": "0.0.1",
  "type": "module",
  "author": "Nishihara Tsubasa",
  "license": "MIT",
  "main": "./dist/index.cjs",
  "module": "./dist/index.js",
  "types": "./dist/types/index.d.ts",
  "files": [
    "dist"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Crayfisher-zari/digital-agency-design-system.git"
  },
  "exports": {
    ".": {
      "import": {
        "default": "./dist/index.js",
        "types": "./dist/types/index.d.ts"
      },
      "require": {
        "default": "./dist/index.umd.cjs",
        "types": "./dist/types/index.d.ts"
      }
    },
    "./style.css": {
      "import": "./dist/style.css",
      "require": "./dist/style.css"
    }
  },
  "scripts": {
    "dev": "vite --host",
    "build": "yarn run vite build && vue-tsc",
    "preview": "vite preview",
    "typeCheck": "yarn run vue-tsc",
    "lint": "eslint \"./**/*.{ts,vue}\" && stylelint \"./**/*.{scss,vue}\" && prettier \"./**/*.{scss,vue,ts}\" --check",
    "lint:fix": "eslint \"./**/*.{vue,ts}\" --fix && stylelint \"./**/*.{scss,vue}\" --fix && prettier \"./**/*.{scss,vue,ts}\" --write",
    "storybook": "storybook dev -p 6006",
    "build-storybook": "storybook build"
  },
  "dependencies": {
    "vue": "3.4.21"
  },
  "devDependencies": {
    "@storybook/addon-essentials": "8.0.6",
    "@storybook/addon-interactions": "8.0.6",
    "@storybook/addon-links": "8.0.6",
    "@storybook/blocks": "8.0.6",
    "@storybook/test": "8.0.6",
    "@storybook/vue3": "8.0.6",
    "@storybook/vue3-vite": "8.0.6",
    "@typescript-eslint/eslint-plugin": "7.5.0",
    "@typescript-eslint/parser": "7.5.0",
    "@vitejs/plugin-vue": "5.0.4",
    "@vue/eslint-config-typescript": "13.0.0",
    "dotenv": "16.4.5",
    "eslint": "8.57.0",
    "eslint-config-prettier": "9.1.0",
    "eslint-plugin-storybook": "^0.8.0",
    "eslint-plugin-vue": "9.24.0",
    "init": "0.1.2",
    "postcss-html": "1.6.0",
    "prettier": "3.2.5",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "sass": "1.74.1",
    "storybook": "8.0.6",
    "storybook-dark-mode": "4.0.1",
    "stylelint": "16.3.1",
    "stylelint-config-prettier": "9.0.5",
    "stylelint-config-recess-order": "5.0.0",
    "stylelint-config-recommended-vue": "1.5.0",
    "stylelint-config-standard-scss": "13.1.0",
    "typescript": "5.4.4",
    "vite": "5.2.8",
    "vue-eslint-parser": "9.4.2",
    "vue-tsc": "2.0.10"
  },
  "resolutions": {
    "jackspeak": "2.1.1"
  },
  "keywords": [
    "JavaScript",
    "TypeScript",
    "vue",
    "ui framework",
    "vue framework"
  ]
}

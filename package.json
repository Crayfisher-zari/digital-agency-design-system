    "eslint-plugin-vue": "9.19.2",
    "init": "0.1.2",
    "postcss-html": "1.5.0",
    "prettier": "3.1.1",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "sass": "1.69.5",
    "storybook": "7.6.5",
    "stylelint": "16.0.2",
    "stylelint-config-prettier": "9.0.5",
    "stylelint-config-recess-order": "4.4.0",
    "stylelint-config-recommended-vue": "1.5.0",
    "stylelint-config-standard-scss": "12.0.0",
    "typescript": "5.3.3",
